{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "tb",
      "kebab-case"
    ],
    "component-selector": [
      true,
      "element",
      "tb",
      "kebab-case"
    ],
    "forin": false,
    "max-line-length": false,
    "prefer-for-of": false,
    "no-string-literal": false,
    "only-arrow-functions": false,
    "curly": false
  }
}
